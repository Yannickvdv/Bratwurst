@model IEnumerable<Bratwurst.Models.Photo>
@{
    ViewBag.Title = "Index";
}

<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<link rel="stylesheet" href="~/Content/main.css" />
<noscript><link rel="stylesheet" href="~/Content/noscript.css" /></noscript>

<div id="main">
    <div id="reel">
        <!-- Header Item -->
        <div id="header" class="item" data-width="400">
            <div class="inner">
                <h1>Bratwurst website</h1>
                <p>
                    Website about our trip to Germany.
                </p>
            </div>
       </div>
        tajtirawrijwaij

        @foreach (var item in Model)
        {
            <article class="item thumb" data-width="282">
                <h2>@item.caption</h2>
                <a href="@item.imageUrl" class="image"><img src="@item.imageUrl" alt="@item.tags"></a>
                <a href="Like">👍🏻</a><a href="Like">👎🏻</a>
            </article>
        }
    </div>
</div>


